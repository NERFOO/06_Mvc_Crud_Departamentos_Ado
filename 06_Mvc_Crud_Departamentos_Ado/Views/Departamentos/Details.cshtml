@model Departamento

<h1>Vista detalles departamento</h1>

<a asp-controller="Departamentos" asp-action="Index" class="btn btn-success">Index</a>

<ul>
    <li class="list-group-item">Id departamento : @Model.IdDepartamento</li>
    <li class="list-group-item">Nombre: @Model.Nombre</li>
    <li class="list-group-item">Localidad : @Model.Localidad</li>
</ul>
